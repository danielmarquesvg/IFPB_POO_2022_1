#ifndef CLIENTE_H_
#define CLIENTE_H_
#include <string>

#include "../POO_Classe_Objeto/Endereco.h"
using namespace std;

using namespace std;

class Cliente {

private:
	string nome;
	Endereco endereco;
	string telefone;

public:
	//construtor
	Cliente(string novoNome, Endereco novoEndereco, string novoTelefone);

	//getters
	string getNome();
	Endereco getEndereco();
	string getTelefone();

	//setters
	void setNome(string novoNome);
	void setEndereco(Endereco novoEndereco);
	void setTelefone(string novoTelefone);
};

#endif /* CLIENTE_H_ */

